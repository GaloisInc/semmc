;; Add immediate, T32, encoding T2 (F7.1.4, F7-2540)
(let
 ((res_nzcv
  ((_ call "df.addWithCarry")
   rDn
   ((_ zero_extend 24)
    imm)
   #x00000000)))
 ((operands
  ((rDn . 'TGPR)
   (imm . 'Imm0_255)))
  (in
   (imm 'CPSR 'PC))
  (defs
   (('PC
    (bvadd 'PC #x00000002))
    ('CPSR
     (ite
      ((_ call "df.testCondition")
       ((_ call "df.ITState")
        'CPSR)
       'CPSR)
      (ite
       (andp
        (notp
         ((_ call "uf.arm.is_r15")
          rDn))
        (notp
         (notp
          (bvne
           #x00
           (concat
            ((_ extract 15 12)
             'CPSR)
            (concat
             ((_ extract 11 10)
              'CPSR)
             ((_ extract 26 25)
              'CPSR)))))))
       (concat
        ((_ extract 35 32)
         (res_nzcv))
        ((_ extract 27 0)
         'CPSR))
       'CPSR)
      'CPSR))
    (rDn
     (ite
      ((_ call "df.testCondition")
       ((_ call "df.ITState")
        'CPSR)
       'CPSR)
      (ite
       (notp
        (bvne
         #x00
         (concat
          ((_ extract 15 12)
           'CPSR)
          (concat
           ((_ extract 11 10)
            'CPSR)
           ((_ extract 26 25)
            'CPSR)))))
       rDn
       ((_ extract 31 0)
        (res_nzcv)))
      rDn))))))
