;; Vector Add & Write Carry Unsigned Quadword (VX-form)
((operands ((vrT . 'Vrrc) (vrA . 'Vrrc) (vrB . 'Vrrc))) 
  (in (vrB vrA 'IP))
  (defs 
    (('VSCR ((_ extract 159 128) ((_ call "undefined") #x000000a0))) 
      (vrT ((_ extract 127 0) ((_ call "undefined") #x000000a0)))
      ('IP (bvadd 'IP #x00000004)))))