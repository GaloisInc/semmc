;; VSX Scalar Convert Single-Precision to Double-Precision Format (XX2-form)
((operands
 ((xT . 'Vsrc)
  (xB . 'Vsrc)))
 (in
  ('FPSCR xB 'IP))
 (defs
  (('FPSCR
   ((_ extract 31 0)
    ((_ call "uf.ppc.vec1")
     "XSCVSPDP"
     xB
     'FPSCR)))
   (xT
    ((_ extract 159 32)
     ((_ call "uf.ppc.vec1")
      "XSCVSPDP"
      xB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x00000004)))))
