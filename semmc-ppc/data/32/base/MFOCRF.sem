;; Move From One Condition Register Field (XFX-form)
((operands
 ((rT . 'Gprc)
  (FXM . 'Crbitm)))
 (in
  ('CR rT FXM 'IP))
 (defs
  ((rT
   (bvand
    ((_ call "df.mask_32")
     (bvmul
      ((_ zero_extend 24)
       FXM)
      #x00000004)
     (bvadd
      (bvmul
       ((_ zero_extend 24)
        FXM)
       #x00000004)
      #x00000003))
    'CR))
   ('IP
    (bvadd 'IP #x00000004)))))
