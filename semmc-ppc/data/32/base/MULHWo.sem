;; Multiply High Word (XO-form, RC=1)
((operands ((rT . 'Gprc) (rA . 'Gprc) (rB . 'Gprc))) 
  (in ('XER 'CR rB rA 'IP))
  (defs 
    (('CR 
       (bvor 
         'CR
         (bvshl 
           ((_ zero_extend 28) 
             (concat 
               (ite 
                 (bvslt 
                   ((_ extract 31 0) (bvmul ((_ sign_extend 32) rA) ((_ sign_extend 32) rB)))
                   #x00000000)
                 #b100
                 (ite 
                   (bvsgt 
                     ((_ extract 31 0) (bvmul ((_ sign_extend 32) rA) ((_ sign_extend 32) rB)))
                     #x00000000)
                   #b010
                   #b001))
               ((_ extract 0 0) 'XER)))
           (bvmul ((_ zero_extend 29) #b000) #x00000004)))) 
      (rT ((_ extract 31 0) (bvmul ((_ sign_extend 32) rA) ((_ sign_extend 32) rB))))
      ('IP (bvadd 'IP #x00000004)))))