;; Move To VSR Word Algebraic (XX1-form)
((operands
 ((xT Vsrc)
  (rA Gprc)))
 (in
  (op.xT loc.IP))
 (defs
  ((op.xT
   (with
    ()
    (concat
     ((_ sign_extend 32)
      ((_ extract 31 0)
       op.rA))
     (call uf.undefinedBV.64))))
   (loc.IP
    (with
     ()
     (bvadd loc.IP #x00000004))))))
