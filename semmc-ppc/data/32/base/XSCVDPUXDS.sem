;; VSX Scalar Truncate Double-Precision Integer and Convert to Unsigned Integer Doubleword Format with Saturate (XX2-form)
((operands ((xT . 'Vsrc) (xB . 'Vsrc)))
 (in ('FPSCR xB 'IP))
 (defs
  (('FPSCR ((_ extract 159 128) ((_ call "ppc.vec1") "XSCVDPUXDS" xB 'FPSCR)))
   (xT ((_ extract 127 0) ((_ call "ppc.vec1") "XSCVDPUXDS" xB 'FPSCR)))
   ('IP (bvadd 'IP #x00000004)))))
