;; Vector Population Count Doubleword (VX-form)
((operands
 ((vrT Vrrc)
  (vrB Vrrc)))
 (in
  (loc.VSCR op.vrB loc.IP))
 (defs
  ((loc.VSCR
   (with
    ()
    ((_ extract 31 0)
     (call uf.ppc.vec1 const.VPOPCNTD op.vrB loc.VSCR))))
   (op.vrT
    (with
     ()
     ((_ extract 159 32)
      (call uf.ppc.vec1 const.VPOPCNTD op.vrB loc.VSCR))))
   (loc.IP
    (with
     ()
     (bvadd loc.IP #x00000004))))))
