;; AND with Complement (X-form, RC=1)
((operands
 ((rA Gprc)
  (rB Gprc)
  (rS Gprc)))
 (in
  (loc.XER loc.CR op.rS op.rB loc.IP))
 (defs
  ((loc.CR
   (with
    ()
    (call
     df.cmpImm
     (bvslt
      (bvand
       op.rS
       (bvnot op.rB))
      #x00000000)
     (bvsgt
      (bvand
       op.rS
       (bvnot op.rB))
      #x00000000)
     #b000
     loc.XER
     loc.CR)))
   (op.rA
    (with
     ()
     (bvand
      op.rS
      (bvnot op.rB))))
   (loc.IP
    (with
     ()
     (bvadd loc.IP #x00000004))))))
