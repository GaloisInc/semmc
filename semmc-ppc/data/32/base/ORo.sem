;; OR (X-form, RC=1)
((operands
 ((rA Gprc)
  (rB Gprc)
  (rS Gprc)))
 (in
  (loc.XER loc.CR op.rS op.rB loc.IP))
 (defs
  ((loc.CR
   (with
    ()
    (call
     df.cmpImm
     (bvslt
      (bvor op.rS op.rB)
      #x00000000)
     (bvsgt
      (bvor op.rS op.rB)
      #x00000000)
     #b000
     loc.XER
     loc.CR)))
   (op.rA
    (with
     ()
     (bvor op.rS op.rB)))
   (loc.IP
    (with
     ()
     (bvadd loc.IP #x00000004))))))
