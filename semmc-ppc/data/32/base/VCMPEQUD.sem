;; Vector Compare Equal To Unsigned Doubleword (VC-form, RC=0)
((operands ((vrT . 'Vrrc) (vrA . 'Vrrc) (vrB . 'Vrrc)))
 (in ('VSCR vrB vrA 'IP))
 (defs
  (('VSCR
    ((_ extract 159 128)
     ((_ call "ppc.vec2")
      "VCMPEQUD"
      vrA
      vrB
      'VSCR)))
   (vrT
    ((_ extract 127 0)
     ((_ call "ppc.vec2")
      "VCMPEQUD"
      vrA
      vrB
      'VSCR)))
   ('IP (bvadd 'IP #x00000004)))))
