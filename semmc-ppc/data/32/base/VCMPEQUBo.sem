;; Vector Compare Equal To Unsigned Byte (VC-form, RC=1)
((operands ((vrT . 'Vrrc) (vrA . 'Vrrc) (vrB . 'Vrrc))) 
  (in ('XER 'CR 'FPSCR vrB vrA 'IP))
  (defs 
    (('CR ((_ call "undefined") #x00000020)) 
      (vrT ((_ extract 127 0) ((_ call "ppc.vec2") (VCMPEQUB) vrA vrB 'FPSCR)))
      ('IP (bvadd 'IP #x00000004)))))