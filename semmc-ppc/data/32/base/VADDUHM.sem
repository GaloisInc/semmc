;; Vector Add Unsigned Halfword Modulo (VX-form)
((operands ((vrT . 'Vrrc) (vrA . 'Vrrc) (vrB . 'Vrrc))) 
 (in ('VSCR vrB vrA 'IP))
 (defs 
  (('VSCR ((_ extract 159 128) ((_ call "ppc.vec2") "VADDUHM" vrA vrB 'VSCR))) 
   (vrT ((_ extract 127 0) ((_ call "ppc.vec2") "VADDUHM" vrA vrB 'VSCR)))
   ('IP (bvadd 'IP #x00000004)))))