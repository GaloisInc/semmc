;; Vector Count Leading Zeros Word (VX-form)
((operands ((vrT . 'Vrrc) (vrB . 'Vrrc)))
 (in ('VSCR vrB 'IP))
 (defs
  (('VSCR
    ((_ extract 159 128)
     ((_ call "ppc.vec1")
      "VCLZW"
      vrB
      'VSCR)))
   (vrT
    ((_ extract 127 0)
     ((_ call "ppc.vec1")
      "VCLZW"
      vrB
      'VSCR)))
   ('IP (bvadd 'IP #x00000004)))))
