;; Store Vector Indexed LRU (X-form)
;; This form provides a cache hint
((operands
 ((memref . 'Memrr)
  (vrS . 'Vrrc)))
 (in
  (memref 'Mem vrS 'IP))
 (defs
  (('Mem
   ((_ call "uf.write_mem.128")
    'Mem
    (bvand
     ((_ sign_extend 24)
      #xfe)
     (bvadd
      ((_ call "uf.ppc.memrr_offset")
       memref)
      (ite
       ((_ call "uf.ppc.is_r0")
        ((_ call "uf.ppc.memrr_base")
         memref))
       #x00000000
       ((_ call "uf.ppc.memrr_base")
        memref))))
    vrS))
   ('IP
    (bvadd 'IP #x00000004)))))
