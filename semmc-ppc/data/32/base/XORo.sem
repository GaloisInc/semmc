;; XOR (X-form, RC=1)
((operands
 ((rA . 'Gprc)
  (rB . 'Gprc)
  (rS . 'Gprc)))
 (in
  ('XER 'CR rB rS 'IP))
 (defs
  (('CR
   ((_ call "df.cmpImm")
    (bvslt
     (bvxor rS rB)
     #x00000000)
    (bvsgt
     (bvxor rS rB)
     #x00000000)
    #b000
    'XER
    'CR))
   (rA
    (bvxor rS rB))
   ('IP
    (bvadd 'IP #x00000004)))))
