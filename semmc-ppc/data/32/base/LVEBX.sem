;; Load Vector Element Byte Indexed (X-form)
((operands
 ((vrT. 'Vrrc)
  (memrr. 'Memrr)))
 (in
  (memrr 'Mem 'IP))
 (defs
  ((vrT
   (concat
    ((_ call "undefined")
     #x00000078)
    ((_ call "read_mem.8")
     'Mem
     (bvadd
      (ite
       ((_ call "ppc.is_r0")
        ((_ call "ppc.memrr_base")
         memrr))
       #x00000000
       ((_ call "ppc.memrr_base")
        memrr))
      ((_ call "ppc.memrr_offset")
       memrr)))))
   ('IP
    (bvadd 'IP #x00000004)))))
