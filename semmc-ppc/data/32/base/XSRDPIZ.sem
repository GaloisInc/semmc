;; VSX Scalar Round to Double-Precision Integer using Round Toward Zero (XX2-form)
((operands ((xT . 'Vsrc) (xB . 'Vsrc))) 
 (in ('FPSCR xB 'IP))
 (defs 
  (('FPSCR ((_ extract 159 128) ((_ call "ppc.vec1") "XSRDPIZ" xB 'FPSCR))) 
   (xT ((_ extract 127 0) ((_ call "ppc.vec1") "XSRDPIZ" xB 'FPSCR)))
   ('IP (bvadd 'IP #x00000004)))))