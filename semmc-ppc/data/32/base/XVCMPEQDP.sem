;; VSX Vector Compare Equal To Double-Precision (XX3-form)
((operands
 ((xT . 'Vsrc)
  (xA . 'Vsrc)
  (xB . 'Vsrc)))
 (in
  ('FPSCR xB xA 'IP))
 (defs
  (('FPSCR
   ((_ extract 159 128)
    ((_ call "ppc.vec2")
     "XVCMPEQDP"
     xA
     xB
     'FPSCR)))
   (xT
    ((_ extract 127 0)
     ((_ call "ppc.vec2")
      "XVCMPEQDP"
      xA
      xB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x00000004)))))
