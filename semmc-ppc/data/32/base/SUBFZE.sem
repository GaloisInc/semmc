;; Subtract From Zero Extended (XO-form, RC=0)
((operands
 ((rT Gprc)
  (rA Gprc)))
 (in
  (loc.XER op.rA loc.IP))
 (defs
  ((op.rT
   (with
    ()
    (bvadd
     (bvnot op.rA)
     ((_ zero_extend 31)
      ((_ extract 29 29)
       loc.XER)))))
   (loc.IP
    (with
     ()
     (bvadd loc.IP #x00000004))))))
