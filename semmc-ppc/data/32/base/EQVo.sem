;; EQV (X-form, RC=1)
((operands
 ((rA . 'Gprc)
  (rS . 'Gprc)
  (rB . 'Gprc)))
 (in
  ('XER 'CR rB rS 'IP))
 (defs
  (('CR
   ((_ call "df.cmpImm")
    (bvslt
     (bvnot
      (bvxor rS rB))
     #x00000000)
    (bvsgt
     (bvnot
      (bvxor rS rB))
     #x00000000)
    #b000
    'XER
    'CR))
   (rA
    (bvnot
     (bvxor rS rB)))
   ('IP
    (bvadd 'IP #x00000004)))))
