;; Vector Compare Equal To Unsigned Halfword (VC-form, RC=0)
((operands
 ((vrT . 'Vrrc)
  (vrA . 'Vrrc)
  (vrB . 'Vrrc)))
 (in
  ('VSCR vrB vrA 'IP))
 (defs
  (('VSCR
   ((_ extract 31 0)
    ((_ call "ppc.vec2")
     "VCMPEQUH"
     vrA
     vrB
     'VSCR)))
   (vrT
    ((_ extract 159 32)
     ((_ call "ppc.vec2")
      "VCMPEQUH"
      vrA
      vrB
      'VSCR)))
   ('IP
    (bvadd 'IP #x00000004)))))
