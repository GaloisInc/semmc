;; VSX Scalar Round to Double-Precision Integer Exact using Current Rounding Mode (XX2-form)
((operands ((xT . 'Vsrc) (xB . 'Vsrc)))
 (in ('FPSCR xB 'IP))
 (defs
  (('FPSCR ((_ extract 159 128) ((_ call "ppc.vec1") "XSRDPIC" xB 'FPSCR)))
   (xT ((_ extract 127 0) ((_ call "ppc.vec1") "XSRDPIC" xB 'FPSCR)))
   ('IP (bvadd 'IP #x00000004)))))
