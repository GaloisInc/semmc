;; Floating Reciprocal Square Root Estimate Single (A-form)
((operands
 ((frT Fprc)
  (frB Fprc)))
 (in
  (loc.FPSCR op.frB loc.IP))
 (defs
  ((loc.FPSCR
   (with
    ()
    ((_ extract 31 0)
     (call uf.ppc.vec1 const.FRSQRTES op.frB loc.FPSCR))))
   (op.frT
    (with
     ()
     ((_ extract 159 32)
      (call uf.ppc.vec1 const.FRSQRTES op.frB loc.FPSCR))))
   (loc.IP
    (with
     ()
     (bvadd loc.IP #x00000004))))))
