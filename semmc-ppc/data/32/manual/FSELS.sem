;; Floating-Point Select Single (A-form)
((operands
 ((frT . 'Fprc)
  (frA . 'Fprc)
  (frB . 'Fprc)
  (frC . 'Fprc)))
 (in
  ('FPSCR frC frB frA 'IP))
 (defs
  (('FPSCR
   ((_ extract 31 0)
    ((_ call "uf.ppc.vec3")
     "FSELS"
     frA
     frB
     frC
     'FPSCR)))
   (frT
    ((_ extract 159 32)
     ((_ call "uf.ppc.vec3")
      "FSELS"
      frA
      frB
      frC
      'FPSCR)))
   ('IP
    (bvadd 'IP #x00000004)))))
