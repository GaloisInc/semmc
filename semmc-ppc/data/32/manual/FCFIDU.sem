;; Floating Point Convert from Integer Doubleword Unsigned (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ call "uf.fp.un_op_fpscr")
    "FCFIDU"
    frB
    'FPSCR))
   (frT
    (concat
     #x0000000000000000
     (fp_double_to_binary
      (fcfidu
       ((_ extract 127 64)
        frB)))))
   ('IP
    (bvadd 'IP #x00000004)))))
