;; Floating Point Convert to Integer Doubleword Unsigned (X-form)
((operands ((frT . 'Fprc) (frB . 'Fprc)))
 (in ('FPSCR frB 'IP))
 (defs
  (('FPSCR ((_ extract 159 128) ((_ call "ppc.vec1") "FCTIDU" frB 'FPSCR)))
   (frT ((_ extract 127 0) ((_ call "ppc.vec1") "FCTIDU" frB 'FPSCR)))
   ('IP (bvadd 'IP #x00000004)))))
