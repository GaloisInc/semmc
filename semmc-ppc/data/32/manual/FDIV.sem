;; Floating Divide (A-form)
((operands ((frT . 'Fprc) (frA . 'Fprc) (frB . 'Fprc))) 
  (in ('FPSCR frB frA 'IP))
  (defs 
    (('FPSCR ((_ extract 159 128) ((_ call "ppc.vec2") "FDIV" frA frB 'FPSCR))) 
      (frT ((_ extract 127 0) ((_ call "ppc.vec2") "FDIV" frA frB 'FPSCR)))
      ('IP (bvadd 'IP #x00000004)))))