;; Floating Point Convert from Integer Doubleword (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ call "uf.fp.un_op_fpscr")
    "FCFID"
    frB
    'FPSCR))
   (frT
    (concat
     (fp_double_to_binary
      (fcfid
       ((_ extract 127 64)
        frB)))
     #x0000000000000000))
   ('IP
    (bvadd 'IP #x00000004)))))
