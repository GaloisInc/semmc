;; Floating Copy Sign (X-form)
((operands
 ((frT . 'Fprc)
  (frA . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB frA 'IP))
 (defs
  (('FPSCR
   ((_ extract 31 0)
    ((_ call "ppc.vec2")
     "FCPSGND"
     frA
     frB
     'FPSCR)))
   (frT
    ((_ extract 159 32)
     ((_ call "ppc.vec2")
      "FCPSGND"
      frA
      frB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x00000004)))))
