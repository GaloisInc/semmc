;; Load Floating-Point Double Indexed (X-form)
((operands ((rT . 'Fprc) (memref . 'Memrr)))
 (in ('Mem memref 'IP))
 (defs
  ((rT
    (concat
     #x0000000000000000
     ((_ call "read_mem.64")
      'Mem
      (bvadd
       (ite
        ((_ call "ppc.is_r0") ((_ call "ppc.memrr_base") memref))
        #x00000000
        ((_ call "ppc.memrr_base") memref))
       ((_ call "ppc.memrr_offset") memref)))))
   ('IP (bvadd 'IP #x00000004)))))
