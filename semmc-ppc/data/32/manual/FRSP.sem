;; Floating Round to Single-Precision (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ call "uf.fp.un_op_fpscr")
    "FRSP"
    frB
    'FPSCR))
   (frT
    (concat
     #x0000000000000000
     ((_ call "uf.fp.double_to_binary")
      (fp_single_to_double
       (frsp
        (fp_binary_to_double
         ((_ extract 127 64)
          frB)))))))
   ('IP
    (bvadd 'IP #x00000004)))))
