;; Floating Point Convert to Integer Doubleword (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ call "uf.fp.un_op_fpscr")
    "FCTID"
    frB
    'FPSCR))
   (frT
    (concat
     #x0000000000000000
     (fctid
      (fp_binary_to_double
       ((_ extract 127 64)
        frB)))))
   ('IP
    (bvadd 'IP #x00000004)))))
