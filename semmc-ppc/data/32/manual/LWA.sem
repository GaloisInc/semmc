;; Load Word Algebraic (DS-form)
((operands ((rT . 'Gprc) (memref . 'Memrix))) 
  (in ('Mem memref 'IP))
  (defs 
    ((rT 
       ((_ call "read_mem.32") 
         'Mem
         (bvadd 
           (ite 
             ((_ call "ppc.is_r0") ((_ call "ppc.memrix_reg") memref))
             #x00000000
             ((_ call "ppc.memrix_reg") memref))
           ((_ sign_extend 16) (concat ((_ call "ppc.memrix_offset") memref) #b00))))) ('IP (bvadd 'IP #x00000004)))))