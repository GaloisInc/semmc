;; Load Word Algebraic (DS-form)
((operands ((rT . 'Gprc) (memref . 'Memri))) 
  (in ('Mem memref))
  (defs 
    ((rT 
       ((_ call "read_mem") 
         'Mem
         (bvadd 
           (ite 
             ((_ call "is_r0") ((_ call memri_reg) memref))
             #x00000000
             ((_ call memri_reg) memref))
           ((_ sign_extend 16) (concat ((_ call "memri_offset") memref) #b00)))
         #x00000004)))))