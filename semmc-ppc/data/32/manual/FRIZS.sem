;; Floating Round to Integer Toward Zero Single (X-form)
((operands
 ((frT. 'Fprc)
  (frB. 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ extract 159 128)
    ((_ call "ppc.vec1")
     "FRIZS"
     frB
     'FPSCR)))
   (frT
    ((_ extract 127 0)
     ((_ call "ppc.vec1")
      "FRIZS"
      frB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x00000004)))))
