;; Store Byte with Update (D-form)
((operands
 ((memref . 'Memri)
  (rS . 'Gprc)))
 (in
  ('Mem memref rS 'IP))
 (defs
  ((((_ call "uf.ppc.memri_reg")
   memref)
   (bvadd
    ((_ call "uf.ppc.memri_reg")
     memref)
    ((_ sign_extend 16)
     ((_ call "uf.ppc.memri_offset")
      memref))))
   ('Mem
    ((_ call "uf.write_mem.8")
     'Mem
     (bvadd
      ((_ call "uf.ppc.memri_reg")
       memref)
      ((_ sign_extend 16)
       ((_ call "uf.ppc.memri_offset")
        memref)))
     ((_ extract 31 24)
      rS)))
   ('IP
    (bvadd 'IP #x00000004)))))
