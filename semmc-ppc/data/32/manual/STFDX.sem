;; Store Floating-Point Double Indexed (X-form)
((operands ((memref . 'Memrr) (frS . 'Fprc)))
 (in ('Mem memref frS 'IP))
 (defs
  (('Mem
    ((_ call "write_mem.64")
     'Mem
     (bvadd
      (ite
       ((_ call "ppc.is_r0")
        ((_ call "ppc.memrr_base")
         memref))
       #x00000000
       ((_ call "ppc.memrr_base") memref))
      ((_ call "ppc.memrr_offset") memref))
     ((_ extract 63 0) frS)))
   ('IP (bvadd 'IP #x00000004)))))
