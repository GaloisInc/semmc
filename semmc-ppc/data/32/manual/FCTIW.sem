;; Floating Point Convert to Integer Word (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ extract 31 0)
    ((_ call "ppc.vec1")
     "FCTIW"
     frB
     'FPSCR)))
   (frT
    ((_ extract 159 32)
     ((_ call "ppc.vec1")
      "FCTIW"
      frB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x00000004)))))
