;; Floating-Point Select (A-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)
  (frC . 'Fprc)
  (frA . 'Fprc)))
 (in
  (frB frC frA 'IP))
 (defs
  ((frT
   (ite
    (fled
     (fp_binary_to_double #x0000000000000000)
     (fp_binary_to_double
      ((_ extract 127 64)
       frA)))
    frC
    frB))
   ('IP
    (bvadd 'IP #x00000004)))))
