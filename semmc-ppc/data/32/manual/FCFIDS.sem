;; Floating Point Convert from Integer Doubleword Single (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ extract 159 128)
    ((_ call "uf.ppc.vec1")
     "FCFIDS"
     frB
     'FPSCR)))
   (frT
    ((_ extract 127 0)
     ((_ call "uf.ppc.vec1")
      "FCFIDS"
      frB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x00000004)))))
