;; Floating Multiply-Subtract Single (A-form)
((operands ((frT . 'Fprc) (frA . 'Fprc) (frB . 'Fprc) (frC . 'Fprc)))
 (in ('FPSCR frC frB frA 'IP))
 (defs
  (('FPSCR ((_ extract 159 128) ((_ call "ppc.vec3") "FMSUBS" frA frB frC 'FPSCR)))
   (frT ((_ extract 127 0) ((_ call "ppc.vec3") "FMSUBS" frA frB frC 'FPSCR)))
   ('IP (bvadd 'IP #x00000004)))))
