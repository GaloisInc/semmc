;; Floating Multiply-Add (A-form)
((operands ((frT . 'Fprc) (frA . 'Fprc) (frB . 'Fprc) (frC . 'Fprc))) 
  (in (frC frB frA 'IP))
  (defs 
    ((frT 
       (concat 
         #x0000000000000000
         ((_ call "fp.muladd64") ((_ extract 63 0) frA) ((_ extract 63 0) frB) ((_ extract 63 0) frC)))) ('IP (bvadd 'IP #x00000004)))))