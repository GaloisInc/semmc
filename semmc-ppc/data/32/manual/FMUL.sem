;; Floating Multiply (A-form)
((operands
 ((frT . 'Fprc)
  (frA . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB frA 'IP))
 (defs
  (('FPSCR
   ((_ call "uf.fp.bin_op_fpscr")
    "FMUL"
    frA
    frB
    'FPSCR))
   (frT
    (concat
     #x0000000000000000
     ((_ call "uf.fp.double_to_binary")
      (fmul
       (fp_binary_to_double
        ((_ extract 127 64)
         frA))
       (fp_binary_to_double
        ((_ extract 127 64)
         frB))))))
   ('IP
    (bvadd 'IP #x00000004)))))
