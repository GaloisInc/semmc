;; Floating Copy Sign (X-form)
((operands
 ((frT . 'Fprc)
  (frA . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB frA 'IP))
 (defs
  (('FPSCR
   ((_ extract 159 128)
    ((_ call "uf.ppc.vec2")
     "FCPSGND"
     frA
     frB
     'FPSCR)))
   (frT
    ((_ extract 127 0)
     ((_ call "uf.ppc.vec2")
      "FCPSGND"
      frA
      frB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
