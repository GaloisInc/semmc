;; Floating Point Convert to Integer Word with Round Towards Zero (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ extract 31 0)
    ((_ call "uf.ppc.vec1")
     "FCTIWZ"
     frB
     'FPSCR)))
   (frT
    ((_ extract 159 32)
     ((_ call "uf.ppc.vec1")
      "FCTIWZ"
      frB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
