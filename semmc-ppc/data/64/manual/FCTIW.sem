;; Floating Point Convert to Integer Word (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ call "uf.fp.un_op_fpscr")
    "FCTIW"
    frB
    'FPSCR))
   (frT
    (concat
     #x0000000000000000
     (concat
      ((_ call "uf.undefined")
       #x00000020)
      (fctiw
       (fp_binary_to_double
        ((_ extract 127 64)
         frB))))))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
