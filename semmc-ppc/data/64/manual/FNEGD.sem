;; Floating Negate (X-form)
;; There is no single-precision form of this because
;; the sign bit is always in the same place (MSB)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ extract 31 0)
    ((_ call "uf.ppc.vec1")
     "FNEGD"
     frB
     'FPSCR)))
   (frT
    ((_ extract 159 32)
     ((_ call "uf.ppc.vec1")
      "FNEGD"
      frB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
