;; Store Byte (D-form)
((operands
 ((memref . 'Memri)
  (rS . 'Gprc)))
 (in
  ('Mem memref rS 'IP))
 (defs
  (('Mem
   ((_ call "uf.write_mem.8")
    'Mem
    (bvadd
     (ite
      ((_ call "uf.ppc.is_r0")
       ((_ call "uf.ppc.memri_reg")
        memref))
      #x0000000000000000
      ((_ call "uf.ppc.memri_reg")
       memref))
     ((_ sign_extend 48)
      ((_ call "uf.ppc.memri_offset")
       memref)))
    ((_ extract 7 0)
     rS)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
