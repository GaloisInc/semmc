;; Floating Negative Absolute Value (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  (frB 'IP))
 (defs
  ((frT
   (concat
    (fp_double_to_binary
     (fp_single_to_double
      (fnegs
       (fabss
        ((_ call "uf.fp.double_to_single")
         (fp_binary_to_double
          ((_ extract 127 64)
           frB)))))))
    #x0000000000000000))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
