;; Floating Square Root (A-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ call "uf.fp.un_op_fpscr")
    "FSQRT"
    frB
    'FPSCR))
   (frT
    (concat
     #x0000000000000000
     (fp_double_to_binary
      (fsqrt
       (fp_binary_to_double
        ((_ extract 127 64)
         frB))))))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
