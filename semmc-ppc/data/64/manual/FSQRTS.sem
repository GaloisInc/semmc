;; Floating Square Root Single (A-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ call "uf.fp.un_op_fpscr")
    "FSQRTS"
    frB
    'FPSCR))
   (frT
    (concat
     (fp_double_to_binary
      (fp_single_to_double
       (fsqrts
        ((_ call "uf.fp.double_to_single")
         (fp_binary_to_double
          ((_ extract 127 64)
           frB))))))
     #x0000000000000000))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
