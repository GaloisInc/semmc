;; Floating Point Convert from Integer Doubleword Unsigned Single (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ call "uf.fp.un_op_fpscr")
    "FCFIDUS"
    frB
    'FPSCR))
   (frT
    (concat
     #x0000000000000000
     ((_ call "uf.fp.double_to_binary")
      (fp_single_to_double
       (fcfidus
        ((_ extract 127 64)
         frB))))))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
