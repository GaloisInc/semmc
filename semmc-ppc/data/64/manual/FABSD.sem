;; Floating Absolute Value (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  (frB 'IP))
 (defs
  ((frT
   (concat
    #x0000000000000000
    ((_ call "uf.fp.double_to_binary")
     (fabsd
      (fp_binary_to_double
       ((_ extract 127 64)
        frB))))))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
