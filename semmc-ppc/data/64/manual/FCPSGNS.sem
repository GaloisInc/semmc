;; Floating Copy Sign Single (X-form)
((operands
 ((frT . 'Fprc)
  (frA . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB frA 'IP))
 (defs
  (('FPSCR
   ((_ extract 31 0)
    ((_ call "uf.ppc.vec2")
     "FCPSGNS"
     frA
     frB
     'FPSCR)))
   (frT
    ((_ extract 159 32)
     ((_ call "uf.ppc.vec2")
      "FCPSGNS"
      frA
      frB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
