;; Floating Copy Sign Single (X-form)
((operands
 ((frT Fprc)
  (frA Fprc)
  (frB Fprc)))
 (in
  (loc.FPSCR op.frB op.frA loc.IP))
 (defs
  ((loc.FPSCR
   (with
    ()
    ((_ extract 31 0)
     (call uf.ppc.vec2 const.FCPSGNS op.frA op.frB loc.FPSCR))))
   (op.frT
    (with
     ()
     ((_ extract 159 32)
      (call uf.ppc.vec2 const.FCPSGNS op.frA op.frB loc.FPSCR))))
   (loc.IP
    (with
     ()
     (bvadd loc.IP #x0000000000000004))))))
