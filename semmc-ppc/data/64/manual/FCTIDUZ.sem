;; Floating Point Convert to Integer Doubleword Unsigned with Round Towards Zero (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   ((_ extract 31 0)
    ((_ call "ppc.vec1")
     "FCTIDUZ"
     frB
     'FPSCR)))
   (frT
    ((_ extract 159 32)
     ((_ call "ppc.vec1")
      "FCTIDUZ"
      frB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
