;; Floating Negate (X-form)
;; There is no single-precision form of this because
;; the sign bit is always in the same place (MSB)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  (frB 'IP))
 (defs
  ((frT
   (concat
    #x0000000000000000
    ((_ call "uf.fp.double_to_binary")
     (fp_single_to_double
      (fnegs
       ((_ call "uf.fp.double_to_single")
        (fp_binary_to_double
         ((_ extract 127 64)
          frB))))))))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
