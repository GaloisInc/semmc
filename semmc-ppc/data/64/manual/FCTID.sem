;; Floating Point Convert to Integer Doubleword (X-form)
((operands
 ((frT . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB 'IP))
 (defs
  (('FPSCR
   (concat
    ((_ call "uf.fp.un_op_fpscr")
     "FCTID"
     frB
     'FPSCR)
    ((_ extract 7 0)
     'FPSCR)))
   (frT
    (concat
     (fctid
      ((_ extract 1 0)
       'FPSCR)
      (fp_binary_to_double
       ((_ extract 127 64)
        frB)))
     #x0000000000000000))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
