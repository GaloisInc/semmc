;; Store Floating-Point Single (D-form)
((operands ((memref . 'Memri) (frS . 'Fprc))) 
 (in ('Mem memref frS 'IP))
 (defs 
  (('Mem 
    ((_ call "write_mem.32") 
     'Mem
     (bvadd 
      (ite 
       ((_ call "ppc.is_r0") ((_ call "ppc.memri_reg") memref))
       #x0000000000000000
       ((_ call "ppc.memri_reg") memref))
      ((_ sign_extend 48) ((_ call "ppc.memri_offset") memref)))
     ((_ call "fp.round_single") ((_ extract 63 0) frS)))) ('IP (bvadd 'IP #x0000000000000004)))))