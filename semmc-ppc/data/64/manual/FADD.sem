;; Floating Add (A-form)
((operands
 ((frT . 'Fprc)
  (frA . 'Fprc)
  (frB . 'Fprc)))
 (in
  ('FPSCR frB frA 'IP))
 (defs
  (('FPSCR
   ((_ call "uf.fp.bin_op_fpscr")
    "FADD"
    frA
    frB
    'FPSCR))
   (frT
    (concat
     #x0000000000000000
     (fp_double_to_binary
      (fadd
       (fp_binary_to_double
        ((_ extract 127 64)
         frA))
       (fp_binary_to_double
        ((_ extract 127 64)
         frB))))))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
