;; Multiply Low Word (XO-form, RC=0)
((operands
 ((rT . 'Gprc)
  (rB . 'Gprc)
  (rA . 'Gprc)))
 (in
  (rA rB 'IP))
 (defs
  ((rT
   ((_ zero_extend 32)
    ((_ extract 31 0)
     (bvmul
      ((_ sign_extend 32)
       ((_ extract 31 0)
        rA))
      ((_ sign_extend 32)
       ((_ extract 31 0)
        rB))))))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
