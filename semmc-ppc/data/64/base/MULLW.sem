;; Multiply Low Word (XO-form, RC=0)
((operands
 ((rT Gprc)
  (rB Gprc)
  (rA Gprc)))
 (in
  (op.rA op.rB loc.IP))
 (defs
  ((op.rT
   (with
    ()
    ((_ zero_extend 32)
     ((_ extract 31 0)
      (bvmul
       ((_ sign_extend 32)
        ((_ extract 31 0)
         op.rA))
       ((_ sign_extend 32)
        ((_ extract 31 0)
         op.rB)))))))
   (loc.IP
    (with
     ()
     (bvadd loc.IP #x0000000000000004))))))
