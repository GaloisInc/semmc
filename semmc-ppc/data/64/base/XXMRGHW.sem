;; VSX Merge High Word (XX3-form)
((operands
 ((xT . 'Vsrc)
  (xA . 'Vsrc)
  (xB . 'Vsrc)))
 (in
  ('FPSCR xB xA 'IP))
 (defs
  (('FPSCR
   ((_ extract 159 128)
    ((_ call "uf.ppc.vec2")
     "XXMRGHW"
     xA
     xB
     'FPSCR)))
   (xT
    ((_ extract 127 0)
     ((_ call "uf.ppc.vec2")
      "XXMRGHW"
      xA
      xB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
