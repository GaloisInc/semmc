;; Vector Unpack Low Signed Word (VX-form)
((operands ((vrT . 'Vrrc) (vrB . 'Vrrc)))
 (in ('VSCR vrB 'IP))
 (defs
  (('VSCR
    ((_ extract 159 128)
     ((_ call "ppc.vec1")
      "VUPKLSW"
      vrB
      'VSCR)))
   (vrT
    ((_ extract 127 0)
     ((_ call "ppc.vec1")
      "VUPKLSW"
      vrB
      'VSCR)))
   ('IP (bvadd 'IP #x0000000000000004)))))
