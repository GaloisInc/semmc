;; Divide Word Unsigned (XO-form, RC=0)
((operands
 ((rT . 'Gprc)
  (rB . 'Gprc)
  (rA . 'Gprc)))
 (in
  (rB rA 'IP))
 (defs
  ((rT
   (bvudiv
    ((_ sign_extend 32)
     ((_ extract 31 0)
      rA))
    ((_ sign_extend 32)
     ((_ extract 31 0)
      rB))))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
