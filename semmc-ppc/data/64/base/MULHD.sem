;; Multiply High Doubleword (XO-form, RC=0)
((operands
 ((rT Gprc)
  (rB Gprc)
  (rA Gprc)))
 (in
  (op.rA op.rB loc.IP))
 (defs
  ((op.rT
   (with
    ()
    ((_ extract 127 64)
     (bvmul
      ((_ sign_extend 64)
       op.rA)
      ((_ sign_extend 64)
       op.rB)))))
   (loc.IP
    (with
     ()
     (bvadd loc.IP #x0000000000000004))))))
