;; VSX Scalar Reciprocal Estimate Single-Precision (XX2-form)
((operands ((xT . 'Vsrc) (xB . 'Vsrc)))
 (in ('FPSCR xB 'IP))
 (defs
  (('FPSCR ((_ extract 159 128) ((_ call "ppc.vec1") "XSREDP" xB 'FPSCR)))
   (xT ((_ extract 127 0) ((_ call "ppc.vec1") "XSREDP" xB 'FPSCR)))
   ('IP (bvadd 'IP #x0000000000000004)))))
