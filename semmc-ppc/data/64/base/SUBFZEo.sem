;; Subtract From Zero Extended (XO-form, RC=1)
((operands
 ((rT . 'Gprc)
  (rA . 'Gprc)))
 (in
  ('XER 'CR 'XER rA 'IP))
 (defs
  (('CR
   ((_ call "df.cmpImm")
    (bvslt
     (bvadd
      (bvnot rA)
      ((_ zero_extend 63)
       ((_ extract 34 34)
        'XER)))
     #x0000000000000000)
    (bvsgt
     (bvadd
      (bvnot rA)
      ((_ zero_extend 63)
       ((_ extract 34 34)
        'XER)))
     #x0000000000000000)
    #b000
    'XER
    'CR))
   (rT
    (bvadd
     (bvnot rA)
     ((_ zero_extend 63)
      ((_ extract 34 34)
       'XER))))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
