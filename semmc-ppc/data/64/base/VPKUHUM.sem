;; Vector Pack Unsigned Halfword Unsigned Modulo (VX-form)
((operands
 ((vrT Vrrc)
  (vrA Vrrc)
  (vrB Vrrc)))
 (in
  (loc.VSCR op.vrB op.vrA loc.IP))
 (defs
  ((loc.VSCR
   (with
    ()
    ((_ extract 31 0)
     (call uf.ppc.vec2 const.VPKUHUM op.vrA op.vrB loc.VSCR))))
   (op.vrT
    (with
     ()
     ((_ extract 159 32)
      (call uf.ppc.vec2 const.VPKUHUM op.vrA op.vrB loc.VSCR))))
   (loc.IP
    (with
     ()
     (bvadd loc.IP #x0000000000000004))))))
