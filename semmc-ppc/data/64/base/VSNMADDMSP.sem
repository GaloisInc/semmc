;; VSX Scalar Negative Multiply-Add Single-Precision (XX3-form)
((operands ((xT . 'Vsrc) (xA . 'Vsrc) (xB . 'Vsrc)))
 (in ('FPSCR xB xA 'IP))
 (defs
  (('FPSCR ((_ extract 159 128) ((_ call "ppc.vec2") "VSNMADDMSP" xA xB 'FPSCR)))
   (xT ((_ extract 127 0) ((_ call "ppc.vec2") "VSNMADDMSP" xA xB 'FPSCR)))
   ('IP (bvadd 'IP #x0000000000000004)))))
