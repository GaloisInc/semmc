;; VSX Vector Copy Sign Single-Precision (XX3-form)
((operands
 ((xT Vsrc)
  (xA Vsrc)
  (xB Vsrc)))
 (in
  (loc.FPSCR op.xB op.xA loc.IP))
 (defs
  ((loc.FPSCR
   (with
    ()
    ((_ extract 31 0)
     (call uf.ppc.vec2 const.XVCPSGNSP op.xA op.xB loc.FPSCR))))
   (op.xT
    (with
     ()
     ((_ extract 159 32)
      (call uf.ppc.vec2 const.XVCPSGNSP op.xA op.xB loc.FPSCR))))
   (loc.IP
    (with
     ()
     (bvadd loc.IP #x0000000000000004))))))
