;; VSX Scalar Round to Double-Precision Integer using Round Toward -Infinity (XX2-form)
((operands
 ((xT . 'Vsrc)
  (xB . 'Vsrc)))
 (in
  ('FPSCR xB 'IP))
 (defs
  (('FPSCR
   ((_ extract 31 0)
    ((_ call "uf.ppc.vec1")
     "XSRDPIM"
     xB
     'FPSCR)))
   (xT
    ((_ extract 159 32)
     ((_ call "uf.ppc.vec1")
      "XSRDPIM"
      xB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
