;; Shift Right Doubleword (X-form, RC=0)
((operands
 ((rA Gprc)
  (rB Gprc)
  (rS Gprc)))
 (in
  (op.rS op.rB loc.IP))
 (defs
  ((op.rA
   (with
    ()
    (bvlshr
     op.rS
     ((_ zero_extend 58)
      ((_ extract 5 0)
       op.rB)))))
   (loc.IP
    (with
     ()
     (bvadd loc.IP #x0000000000000004))))))
