;; Vector Population Count Doubleword (VX-form)
((operands
 ((vrT . 'Vrrc)
  (vrB . 'Vrrc)))
 (in
  ('VSCR vrB 'IP))
 (defs
  (('VSCR
   ((_ extract 159 128)
    ((_ call "ppc.vec1")
     "VPOPCNTD"
     vrB
     'VSCR)))
   (vrT
    ((_ extract 127 0)
     ((_ call "ppc.vec1")
      "VPOPCNTD"
      vrB
      'VSCR)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
