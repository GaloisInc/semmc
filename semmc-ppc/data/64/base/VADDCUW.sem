;; Vector Add and Write Carry-Out Unsigned Word (VX-form)
((operands ((vrT . 'Vrrc) (vrA . 'Vrrc) (vrB . 'Vrrc))) 
  (in ('FPSCR vrB vrA 'IP))
  (defs 
    (('FPSCR ((_ extract 159 128) ((_ call "ppc.vec2") (VPERM) vrA vrB 'FPSCR))) 
      (vrT ((_ extract 127 0) ((_ call "ppc.vec2") (VPERM) vrA vrB 'FPSCR)))
      ('IP (bvadd 'IP #x0000000000000004)))))