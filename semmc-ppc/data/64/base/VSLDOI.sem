  (vrA . 'Vrrc)
  (vrB . 'Vrrc)))
 (in
  ('VSCR vrB vrA 'IP))
 (defs
  (('VSCR
   ((_ extract 31 0)
    ((_ call "ppc.vec3")
     "VSLDOI"
     ((_ zero_extend 123)
