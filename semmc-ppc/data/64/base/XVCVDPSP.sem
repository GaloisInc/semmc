;; VSX Vector Round Double-Precision to Single-Precision and Convert to Single-Precision Format (XX2-form)
((operands
 ((xT . 'Vsrc)
  (xB . 'Vsrc)))
 (in
  ('FPSCR xB 'IP))
 (defs
  (('FPSCR
   ((_ extract 31 0)
    ((_ call "uf.ppc.vec1")
     "XVCVDPSP"
     xB
     'FPSCR)))
   (xT
    ((_ extract 159 32)
     ((_ call "uf.ppc.vec1")
      "XVCVDPSP"
      xB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
