;; OR with Complement (X-form, RC=1)
((operands
 ((rA . 'Gprc)
  (rS . 'Gprc)
  (rB . 'Gprc)))
 (in
  ('XER 'CR rB rS 'IP))
 (defs
  (('CR
   ((_ call "df.cmpImm")
    (bvslt
     (bvor
      rS
      (bvnot rB))
     #x0000000000000000)
    (bvsgt
     (bvor
      rS
      (bvnot rB))
     #x0000000000000000)
    #b000
    'XER
    'CR))
   (rA
    (bvor
     rS
     (bvnot rB)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
