;; Vector Compare Equal To Unsigned Halfword (VC-form, RC=1)
((operands
 ((vrT . 'Vrrc)
  (vrA . 'Vrrc)
  (vrB . 'Vrrc)))
 (in
  ('XER 'CR 'VSCR vrB vrA 'IP))
 (defs
  (('CR
   ((_ call "df.updateCRField")
    'CR
    #b110
    ((_ call "uf.undefined")
     #x00000004)))
   ('VSCR
    ((_ extract 159 128)
     ((_ call "uf.ppc.vec2")
      "VCMPEQUH"
      vrA
      vrB
      'VSCR)))
   (vrT
    ((_ extract 127 0)
     ((_ call "uf.ppc.vec2")
      "VCMPEQUH"
      vrA
      vrB
      'VSCR)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
