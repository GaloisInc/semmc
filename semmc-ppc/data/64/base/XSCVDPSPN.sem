;; VSX Scalar Convert Scalar Single-Precision to Vector Single-Precision Format Non-Signaling (XX2-form)
((operands
 ((xT . 'Vsrc)
  (xB . 'Vsrc)))
 (in
  ('FPSCR xB 'IP))
 (defs
  (('FPSCR
   ((_ extract 31 0)
    ((_ call "uf.ppc.vec1")
     "XSCVDPSPN"
     xB
     'FPSCR)))
   (xT
    ((_ extract 159 32)
     ((_ call "uf.ppc.vec1")
      "XSCVDPSPN"
      xB
      'FPSCR)))
   ('IP
    (bvadd 'IP #x0000000000000004)))))
