[fuzzer]
arch = ppc32
opcodes = AllOpcodes
strategy = Randomized
# Assuming that the fuzzermon service is running on port 8000
report-url = http://localhost:8000

[host:arm.example.com]
# This is the path to the remote runner binary on the remote machine
runner-path = /path/to/remote-runner
chunk-size = 100
threads = 1
